                },
                "org_name": {
                    "type": "string",
                    "description": "Name of the Aramark Organization"
                },
                "check_count": {
                    "type": "number",
                    "description": "Number of checks"
                },
                "mashgin_checks": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/mashgin_checks"
                    }
                }
            }
        },
        "items": {
