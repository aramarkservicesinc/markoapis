                },
                "org_name": {
                    "type": "string",
                    "description": "Name of hierarchy element"
                },
                "id": {
                    "type": "string",
                    "description": "ID of hierarchy element"
                },
                "node_type": {
                    "type": "string",
                    "description": "Level of the hierarchy for node"
                }
            }
        },
        "level": {
            "type": "object",
            "properties": {
                "org_value": {
                    "type": "string",
