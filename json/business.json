                },
                "children": {
                    "type": "string",
                    "description": "True or False if organization has children"
                },
                "org_parent": {
                    "type": "string",
                    "description": "Aramark Organization code of level parent"
                },
                "text": {
                    "type": "string",
                    "description": "Text value of organization value"
                },
                "state": {
                    "type": "object",
                    "properties": {
