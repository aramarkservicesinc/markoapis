{
    "openapi": "3.0.0",
    "info": {
        "summary": "This API will return Business Hierarchy data",
        "description": "\nReturns hierarchical information about Aramark's business organization.\n\nThe base endpoint URL for this API in production is: `https://marko.aramark.net/v1/business`.\n\nFor testing the API with the \"Try\" tool on this page or in your app, use either our testing server \"qa-marko\" or our development server \"dev-marko\".\n",
        "version": "1.0.0-oas3",
        "title": "Business"
    },
    "tags": [
        {
            "name": "Hierarchy",
            "description": "Returns Hierarchical Data"
        }
    ],
    "paths": {
        "/hierarchies": {
            "get": {
                "description": "\nReturns a collection of all visible hierarchies\n\n![This endpoint is in development](/themes/portal/images/statuspngs/statusdev.png)\n",
                "operationId": "getAllHierarchies",
                "tags": [
                    "Hierarchy"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/bypassCacheParam"
                    },
                    {
                        "$ref": "#/components/parameters/smokeParam"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Provides the Financial Account Summary for a specific Organization",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "description": "ENUM 'Success', 'Error' or 'Not Found'",
                                            "default": "Success"
                                        },
                                        "count": {
                                            "type": "string",
                                            "description": "Number of records returned",
                                            "default": "0"
                                        },
                                        "financial_summary": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/hierarchy_elements"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Requested resource does not exist."
                    },
                    "405": {
                        "description": "HTTP Verb / Operation not supported"
                    }
                }
            }
        },
        "/hierarchy": {
            "get": {
                "description": "Returns a single hierarchy",
                "operationId": "getHierarchy",
                "tags": [
                    "Hierarchy"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/hierarchyNameParam"
                    },
                    {
                        "$ref": "#/components/parameters/bypassCacheParam"
                    },
                    {
                        "$ref": "#/components/parameters/smokeParam"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Provides the Financial Account Summary for a specific Organization",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "description": "ENUM 'Success', 'Error' or 'Not Found'",
                                            "default": "Success"
                                        },
                                        "count": {
                                            "type": "string",
                                            "description": "Number of records returned",
                                            "default": "0"
                                        },
                                        "hierarchy": {
                                            "type": "object",
                                            "description": "List of elements in specified hierarchy"
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Requested resource does not exist."
                    },
                    "405": {
                        "description": "HTTP Verb / Operation not supported"
                    }
                }
            }
        },
        "/hierarchy/expand": {
            "get": {
                "description": "Returns expanded data of a particular node",
                "operationId": "getExpandedHierarchy",
                "tags": [
                    "Hierarchy"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/hierarchyNameParam"
                    },
                    {
                        "in": "query",
                        "name": "level",
                        "description": "Level name to be expanded in hierarchy",
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "$ref": "#/components/parameters/bypassCacheParam"
                    },
                    {
                        "$ref": "#/components/parameters/smokeParam"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Returns the data of a particular node",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "description": "ENUM 'Success', 'Error' or 'Not Found'",
                                            "default": "Success"
                                        },
                                        "count": {
                                            "type": "string",
                                            "description": "Number of records returned",
                                            "default": "0"
                                        },
                                        "levels": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/level"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Requested resource does not exist."
                    },
                    "405": {
                        "description": "HTTP Verb / Operation not supported"
                    }
                }
            }
        },
        "/hierarchy/details": {
            "get": {
                "description": "Returns hierarchy element details based on search term",
                "operationId": "getHierarchyDetails",
                "tags": [
                    "Hierarchy"
                ],
                "parameters": [
                    {
                        "$ref": "#/components/parameters/hierarchyNameParam"
                    },
                    {
                        "in": "query",
                        "name": "search",
                        "description": "String value to be serached for",
                        "required": true,
                        "schema": {
                            "type": "string"
                        }
                    },
                    {
                        "$ref": "#/components/parameters/bypassCacheParam"
                    },
                    {
                        "$ref": "#/components/parameters/smokeParam"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Provides the hierarchy element details based on a search term",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "status": {
                                            "type": "string",
                                            "description": "ENUM 'Success', 'Error' or 'Not Found'",
                                            "default": "Success"
                                        },
                                        "count": {
                                            "type": "string",
                                            "description": "Number of records returned",
                                            "default": "0"
                                        },
                                        "hierarchy_name": {
                                            "type": "string",
                                            "description": "Abbreviated name of the selected hierarchy"
                                        },
                                        "hierarchy_elements": {
                                            "type": "array",
                                            "items": {
                                                "$ref": "#/components/schemas/hierarchy_elements"
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Requested resource does not exist."
                    },
                    "405": {
                        "description": "HTTP Verb / Operation not supported"
                    }
                }
            }
        },
