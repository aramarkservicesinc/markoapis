    "servers": [
        {
            "url": "https://dev-marko.aramark.net/v1/business"
        }
    ],
    "components": {
        "parameters": {
            "hierarchyNameParam": {
                "in": "query",
                "name": "name",
                "description": "Hierarchy Name abbreviation:\n\n * `FH` - Financial Hierarchy \n * `CFH` - Client Family Hierarchy \n * `SH` - Site Hierarchy \n * `GH` - Global Hierarchy\n * `CCHB` - Client Combines Hierarchy\n * `RSRCH` - Reporting Combines Hierarchy\n * `GPOH` - GPO Hierarchy\n",
                "schema": {
                    "type": "string",
                    "enum": [
                        "FH",
                        "CFH",
                        "SH",
                        "GH",
                        "CCBH",
                        "RSRCH",
                        "GPOH"
                    ]
                }
            },
            "bypassCacheParam": {
                "in": "header",
                "name": "bypass-cache",
                "description": "This HTTP Header variable will bypass cache.",
                "schema": {
                    "type": "string",
                    "enum": [
                        "true",
                        "false"
                    ],
                    "default": false
                }
            },
            "smokeParam": {
                "in": "header",
                "name": "smoke",
                "description": "When set to 'true' this HTTP header variable will route the API call to the Integration testing environment.  Only applicable in QA.",
                "schema": {
                    "type": "string",
                    "default": false
                }
            }
        },
        "securitySchemes": {
            "apiKey": {
                "type": "apiKey",
                "in": "header",
                "name": "apikey",
                "description": "This HTTP Header variable will verify permissions."
            }
        },
        "schemas": {
            "hierarchy": {
                "type": "object",
                "properties": {
                    "hierarchy_name": {
                        "type": "string",
                        "description": "Name of hierarchy"
                    },
                    "is_expand": {
                        "type": "string",
                        "description": "True or False determining if hierarchy is expandable"
                    },
                    "hierachy_abbreviation": {
                        "type": "string",
                        "description": "Abbreviation code for a hierarchy"
                    },
                    "input_text": {
                        "type": "string",
                        "description": "Description for input text"
                    },
                    "levels": {
                        "type": "object",
                        "properties": {
                            "name": {
                                "type": "string",
                                "description": "Name of hierarchy level"
                            },
                            "is_level_expand": {
                                "type": "string",
                                "description": "True or False determining if hierarchy level is expandable"
                            },
                            "tree_id": {
                                "type": "string",
                                "description": "ID of tree"
                            }
                        }
                    }
                }
            },
            "hierarchy_elements": {
                "type": "object",
                "properties": {
                    "org_value": {
                        "type": "string",
                        "description": "Aramark Organization code"
                    },
                    "org_name": {
                        "type": "string",
                        "description": "Name of hierarchy element"
                    },
                    "id": {
                        "type": "string",
                        "description": "ID of hierarchy element"
                    },
                    "node_type": {
                        "type": "string",
                        "description": "Level of the hierarchy for node"
                    }
                }
            },
            "level": {
                "type": "object",
                "properties": {
                    "org_value": {
                        "type": "string",
                        "description": "Aramark Organization code"
                    },
                    "children": {
                        "type": "string",
                        "description": "True or False if organization has children"
                    },
                    "org_parent": {
                        "type": "string",
                        "description": "Aramark Organization code of level parent"
                    },
                    "text": {
                        "type": "string",
                        "description": "Text value of organization value"
                    },
                    "state": {
                        "type": "object",
                        "properties": {
                            "opened": {
                                "type": "string",
                                "description": "True or False if organization is opened"
                            }
                        }
                    }
                }
            }
        }
    }
}